{
  "private": true,
  "scripts": {
    "dev": "concurrently \"yarn:cl:dev\" \"next\"",
    "build": "yarn run cc && next build",
    "start": "next start",
    "lint": "next lint",
    "server": "http-server",
    "cl:dev": "contentlayer dev",
    "cl:build": "yarn run cc && contentlayer build",
    "format": "prettier --check . --write \"*.{js,ts,tsx,jsx}\"",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "md2json:global": "markdown-json -c ./markdown-to-json/settings/global.json",
    "md2json:qios": "markdown-json -c ./markdown-to-json/settings/qios.json",
    "md2json:kata-omnichat": "markdown-json -c ./markdown-to-json/settings/kata-omnichat.json",
    "md2json:kata-platform": "markdown-json -c ./markdown-to-json/settings/kata-platform.json",
    "md2json:business-dashboard": "markdown-json -c ./markdown-to-json/settings/business-dashboard.json",
    "md2json:tutorials": "markdown-json -c ./markdown-to-json/settings/tutorials.json",
    "md2json:convert": "yarn run md2json:global && yarn run md2json:qios && yarn run md2json:kata-omnichat && yarn run md2json:kata-platform && yarn run md2json:business-dashboard && yarn run md2json:tutorials",
    "md2json:sanitize": "node ./scripts/sanitize.js",
    "md2json:all": "yarn run md2json:convert && yarn run md2json:sanitize && yarn run format",
    "linking": "node ./scripts/linkingToc.js",
    "cc": "rimraf .contentlayer"
  },
  "dependencies": {
    "@aksara-ui/icons": "1.0.1-canary.1",
    "@aksara-ui/react": "1.0.1-canary.35",
    "@svgr/webpack": "^6.1.2",
    "babel-preset-next": "^1.4.0",
    "classnames": "2.2.6",
    "contentlayer": "^0.1.0",
    "date-fns": "^2.28.0",
    "fuse.js": "^6.5.3",
    "gray-matter": "4.0.2",
    "htmr": "^1.0.1",
    "next": "latest",
    "next-contentlayer": "^0.1.0",
    "react": "latest",
    "react-dom": "^17.0.2",
    "react-lite-youtube-embed": "^2.2.0",
    "react-scrollspy-nav": "^1.2.4",
    "sass": "^1.45.2",
    "string": "^3.3.3",
    "styled-components": "^5.3.3",
    "tslib": "^2.3.1",
    "utility-types": "^3.10.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^12.0.10",
    "@styled-system/should-forward-prop": "^5.1.5",
    "@types/react": "^17.0.37",
    "@types/string": "^0.0.31",
    "@types/styled-components": "^5.1.19",
    "@types/styled-system__should-forward-prop": "^5.1.2",
    "autoprefixer": "^10.2.1",
    "babel-plugin-styled-components": "^2.0.2",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.5.0",
    "eslint-config-next": "12.0.10",
    "http-server": "^14.0.0",
    "image-size": "^1.0.1",
    "markdown-json": "^1.6.1",
    "postcss": "^8.2.4",
    "prettier": "^2.5.1",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-prism-plus": "^1.3.1",
    "rehype-raw": "^6.1.1",
    "rehype-slug": "^5.0.1",
    "remark-gfm": "^3.0.1",
    "remark-rehype": "^10.1.0",
    "rimraf": "^3.0.2",
    "sanitize-html": "^2.6.1",
    "tailwindcss": "^2.0.2",
    "typescript": "^4.5.3",
    "unified": "^10.1.1"
  },
  "resolutions": {
    "@mdx-js/react": "1.6.22"
  }
}
